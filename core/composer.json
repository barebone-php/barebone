  "minimum-stability" : "dev",
  "prefer-stable" : true,
  "require": {
    "php": ">=5.5.0",
    "illuminate/database": "^5.3",
    "hassankhan/config": "^0.10.0",
    "illuminate/events": "^5.3",
    "slim/slim": "^3.5",
    "philo/laravel-blade": "^3.1",
    "monolog/monolog": "^1.21",
    "aura/session": "^2.0",
    "whatthejeff/nyancat-phpunit-resultprinter": "^1.2",
    "robmorgan/phinx": "^0.6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "5.5.*"
